{
  "name": "navno-vite-preact-template-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server/server.js",
    "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -w ./dist/server ./dist/server/server.js\""
  },
  "dependencies": {
    "compression": "1.7.4",
    "csp-header": "5.2.1",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "lru-cache": "10.0.1",
    "node-cache": "5.1.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.4",
    "@types/express": "4.17.20",
    "@types/node": "20.8.10",
    "concurrently": "8.2.2",
    "nodemon": "3.0.1"
  },
  "nodemonConfig": {
    "delay": 500
  }
}
